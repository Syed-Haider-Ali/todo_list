{
    "builds":[{
        "src":"todo_list/wsgi.py",
        "use":"@vercel/python",
        "config": {
            "maxLambdaSize":"15mb", 
            "runtime": "python3.10.5"
         }
    }],
    "routes": [
        {
            "src":"/(.*)",
            "dest": "todo_list/wsgi.py"
        }
    ]
}